<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.user.myprojectpractice">

    <uses-permission android:name="android.permission.CALL_PHONE" />
    <uses-permission android:name="android.permission.INTERNET" />
    <uses-permission android:name="android.permission.ACCESS_WIFI_STATE" />
    <uses-permission android:name="android.permission.ACCESS_NETWORK_STATE" />

    <application
        android:allowBackup="true"
        android:icon="@drawable/chef"
        android:label="Restaurant Information"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".Splashscreen">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".MainActivity" />
        <activity android:name=".SearchRestaurant" />
        <activity android:name=".LoginActivity" />
        <activity android:name=".SigningActivity" />
        <activity android:name=".ReviewActivity" />
        <activity android:name=".RestaurantsActivity" />
        <activity android:name=".ResDetails" />
        <activity android:name=".RestaurantReview" />
        <activity android:name=".InstructionsActivity"></activity>
    </application>

</manifest>